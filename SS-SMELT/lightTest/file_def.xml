    <!--
============================================================================================================
=                                           output files definition                                        =
=                                            Define your own files                                         =
=                                         put the variables you want...                                    =
============================================================================================================
    -->
    <file_definition type="one_file" name="@expname@_@freq@_@startdate@_@enddate@" sync_freq="1d" min_digits="4">
      <file_group id="1ts" output_freq="1ts"  output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 1 time step files -->
      <file_group id="1h" output_freq="1h"  output_level="10" compression_level="4" enabled=".TRUE."> <!-- 1h files -->
    	<file id="file1" name_suffix="_grid_T" description="ocean T grid variables" >
          <field field_ref="ssh"            name="sossheig"/>
          <field field_ref="toce"           name="votemper"/>
          <field field_ref="soce"           name="vosaline"/>
          <field field_ref="mldr10_1"       name="mixed_depth"/>
        </file>
    	<file id="file2" name_suffix="_grid_U" description="ocean U grid variables" >
          <field field_ref="uoce"           name="vozocrtx"  />
        </file>
	    <file id="file3" name_suffix="_grid_V" description="ocean V grid variables" >
          <field field_ref="voce"           name="vomecrty"  />
        </file>
	    <file id="file4" name_suffix="_grid_W" description="ocean W grid variables" >
          <field field_ref="woce"           name="vovecrtz" />
          <field field_ref="avt"            name="vert_eddy_diff" />
          <field field_ref="avm"            name="vert_eddy_visc" />
          <field field_ref="eps" name="dissipation"/>
        </file>
        <file id="file5" name_suffix="_ptrc_T" description="biogeochemical variables" >
          <field field_ref="NO3"            name="nitrate"/>
          <field field_ref="NH4"            name="ammonium"/>
          <field field_ref="Si"             name="silicon"/>
          <field field_ref="DIAT"           name="diatoms"/>
          <field field_ref="PHY"            name="flagellates"/>
          <field field_ref="MYRI"           name="ciliates"/>
          <field field_ref="MICZ"           name="microzooplankton"/>
          <field field_ref="DON"            name="dissolved_organic_nitrogen"/>
          <field field_ref="PON"            name="particulate_organic_nitrogen"/>
          <field field_ref="bSi"            name="biogenic_silicon"/>
          <field field_ref="TRA"            name="Fraser_tracer"/>
          <field field_ref="MESZ"           name="mesozooplankton"/>
        </file>
     </file_group>
      <file_group id="2h" output_freq="2h"  output_level="10" compression_level="4" enabled=".TRUE."> <!-- 2h files -->
        <file id="file6" name_suffix="_dia1_T" description="smelt diagnostics A" >
          <field field_ref="PAR"      />
          <field field_ref="PPDIAT"      />
          <field field_ref="PPPHY"       />
          <field field_ref="PPMRUB"     />
          <field field_ref="PPDIATNO3"      />
          <field field_ref="PPPHYNO3"     />
          <field field_ref="PPMRUBNO3"     />
          <field field_ref="HetMRub"      />
          <field field_ref="MORTPHY"      />
          <field field_ref="MORTMRUB"      />
          <field field_ref="MORTDIAT"      />
          <field field_ref="TQ10"       />
        </file>
     </file_group>
      <file_group id="3h" output_freq="3h"  output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 3h files -->
      <file_group id="4h" output_freq="4h"  output_level="10" compression_level="4" enabled=".FALSE."/> <!-- 4h files -->
      <file_group id="6h" output_freq="6h"  output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 6h files -->

      <file_group id="1d" output_freq="1d"  output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 1d files -->
      <file_group id="3d" output_freq="3d"  output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 3d files -->
      <file_group id="5d" output_freq="5d"  output_level="10" compression_level="4" enabled=".TRUE."/>  <!-- 5d files -->

      <file_group id="1m" output_freq="1mo" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real monthly files -->
      <file_group id="2m" output_freq="2mo" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 2m files -->
      <file_group id="3m" output_freq="3mo" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 3m files -->
      <file_group id="4m" output_freq="4mo" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 4m files -->
      <file_group id="6m" output_freq="6mo" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 6m files -->

      <file_group id="1y"  output_freq="1y" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real yearly files -->
      <file_group id="2y"  output_freq="2y" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 2y files -->
      <file_group id="5y"  output_freq="5y" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 5y files -->
      <file_group id="10y" output_freq="10y" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- real 10y files -->

      <file_group id="20min" output_freq="1200s" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 20 minute files -->
      <file_group id="10min" output_freq="600s" output_level="10" compression_level="4" enabled=".TRUE."/> <!-- 10 minute files -->

   </file_definition>
